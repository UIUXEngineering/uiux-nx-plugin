{
  "name": "uiux-nx-plugin",
  "version": "0.0.15",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "rm -rf dist && npx nx run-many --target=build --all",
    "test": "nx test",
    "ts-node:config": "ts-node --showConfig",
    "update:versions": "ts-node ./build/update-version-numbers.ts",
    "build:ng-prototype": "npx nx run nx-ng-mat-prototype:build && npx nx run nx-ng-mat-prototype:build",
    "build:schematics": "npx nx run schematics:build",
    "publish:next": "ts-node ./build/publish-next.ts",
    "publish:latest": "ts-node ./build/publish-latest.ts",
    "update:schematic:deps": "node ./scripts/update-schematic-dependencies.js",
    "chmod": "node ./scripts/chmod packages/nx-ng-mat-prototype/bin/generate.sh"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/schematics": "^14.2.6",
    "@nrwl/cli": "15.0.0",
    "@nrwl/devkit": "^15.0.0",
    "@nrwl/eslint-plugin-nx": "15.0.0",
    "@nrwl/jest": "15.0.0",
    "@nrwl/js": "15.0.0",
    "@nrwl/linter": "15.0.0",
    "@nrwl/nx-plugin": "^15.0.0",
    "@nrwl/workspace": "15.0.0",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cpy-cli": "^4.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "fs-extra": "^10.1.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "15.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "sorcery": "^0.10.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "dependencies": {
    "@angular-architects/ddd": "^2.1.2",
    "@angular/material": "^14.2.5",
    "@nrwl/angular": "^15.0.0",
    "@swc/helpers": "~0.4.11",
    "tslib": "^2.3.0"
  }
}
