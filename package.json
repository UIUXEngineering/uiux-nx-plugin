  "scripts": {
    "start": "nx serve",
    "build": "rm -rf dist && npx nx run-many --target=build --all",
    "test": "npx nx run-many --target=test --all",
    "ts-node:config": "ts-node --showConfig",
    "update:versions": "ts-node ./build/update-version-numbers.ts",
    "build:ng-prototype": "npx nx run nx-ng-mat-prototype:build",
    "schematics:build": "npx nx run schematics:build",
    "charts:build": "npx nx run charts:build",
    "fn:build": "npx nx run fn:build",
    "fn:test": "npx nx run fn:test",
    "date:build": "npx nx run date:build",
    "date:test": "npx nx run date:test",
    "charts:test": "npx nx run charts:test",
    "publish:next": "ts-node ./build/publish-next.ts",
    "publish:latest": "ts-node ./build/publish-latest.ts",
    "update:schematic:deps": "node ./scripts/update-schematic-dependencies.js",
    "chmod": "node ./scripts/chmod packages/nx-ng-mat-prototype/bin/generate.sh"
  },
  "private": true,
